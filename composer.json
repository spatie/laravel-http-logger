{
    "name": "spatie/laravel-http-logger",
    "description": "A Laravel 5 package to log HTTP requests",
    "keywords": [
        "spatie",
        "laravel-http-logger"
    ],
    "homepage": "https://github.com/spatie/laravel-http-logger",
    "license": "MIT",
    "authors": [
        {
            "name": "Brent Roose",
            "email": "brent@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "~5.6.0|~5.7.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.6.0|~3.7.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\HttpLogger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\HttpLogger\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\HttpLogger\\HttpLoggerServiceProvider"
            ]
        }
    }
}
