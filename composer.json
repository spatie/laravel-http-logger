{
    "name": "spatie/laravel-http-logger",
    "description": "A Laravel package to log HTTP requests",
    "keywords": [
        "spatie",
        "laravel-http-logger"
    ],
    "homepage": "https://github.com/spatie/laravel-http-logger",
    "license": "MIT",
    "authors": [
        {
            "name": "Brent Roose",
            "email": "brent@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "pestphp/pest": "^1.22",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\HttpLogger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\HttpLogger\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\HttpLogger\\HttpLoggerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
